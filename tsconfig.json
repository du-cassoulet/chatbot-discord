{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"rootDir": "./src",
		"resolveJsonModule": true,
		"outDir": "./out",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"strictPropertyInitialization": false,
		"skipLibCheck": true
	}
}
